{
    "$schema": "http://json.schemastore.org/prettierrc",
    "tabWidth": 4,
    "quoteProps": "preserve",
    "semi": true,
    "arrowParens": "always",
    "printWidth": 120,
    "useTabs": false,
    "trailingComma": "all",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "singleQuote": false,
    "bracketSameLine": false
}
